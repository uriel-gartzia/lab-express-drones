<div class="main">

    <div class="content">

 
<h1>Available drones</h1>

<div class="allDrones">

    {{#each this.allDrones}}
    <div class="drone">
    <h3>{{this.name}}</h3>
    <p>{{this.propellers}}</p>
    <p>{{this.maxSpeed}}</p>
    <p><a href="/drones/{{this._id}}/edit">Edit</a></p>
    <form action="/drones/{{this._id}}/delete" method="post"><button>Delete</button></form>
    </div>
    {{/each}}
    </div>

   </div>
</div>
